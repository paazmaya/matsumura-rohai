{
  "rules": {
    "block-scoped-var": 0,
    "brace-style": ["2", "stroustrup"],
    "camelcase": 0,
    "complexity": [0, 12],
    "consistent-return": 2,
    "consistent-this": [2, "self"],
    "curly": 2,
    "dot-notation": 1,
    "eqeqeq": 2,
    "func-names": 1,
    "func-style": [0, "declaration"],
    "guard-for-in": 1,
    "max-depth": [0, 4],
    "max-len": [0, 120, 4],
    "max-nested-callbacks": [0, 2],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 2,
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-comma-dangle": 2,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 0,
    "no-dupe-keys": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-empty-label": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-extra-strict": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-func-assign": 2,
    "global-strict": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": [0, false],
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 0,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-plusplus": 0,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 0,
    "no-sequences": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 1,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-underscore-dangle": 1,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, "local"],
    "no-use-before-define": 1,
    "no-with": 2,
    "no-wrap-func": 2,
    "yoda": "never",
    "one-var": 0,
    "quote-props": 0,
    "quotes": [2, "single"],
    "radix": 1,
    "semi": 2,
    "sort-vars": 0,
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-word-ops": 0,
    "strict": 2,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "wrap-iife": 0,
    "wrap-regex": 0,

    "no-undefined": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "eol-last": 1
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "$": false,
    "jQuery": false,
    "Hogan": false,
    "request": false
  }
}
